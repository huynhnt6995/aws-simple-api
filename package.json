    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/uuid": "^9.0.1",
    "serverless-webpack": "^5.11.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3"
  },
  "build": "tsc",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "deploy": "serverless deploy --stage dev3 --aws-profile huynhnt",
