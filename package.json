  "keywords": [
    "aws",
    "express",
    "nodejs",
    "lambda",
    "simple",
    "serverless"
  ],
  "dependencies": {
    "@aws-sdk/lib-dynamodb": "^3.303.0",
    "dynamoose": "^3.1.0",
    "express": "^4.18.2",
